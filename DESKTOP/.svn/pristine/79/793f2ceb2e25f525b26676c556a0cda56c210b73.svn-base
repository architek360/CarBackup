/*------------------------------------*\
    $GLOBAL STYLES
\*------------------------------------*/
* { outline: normal;

}
:focus {
    outline: auto 5px -webkit-focus-ring-color;
}
body {
	background-color:$primary-1;
}

html {
	-webkit-text-size-adjust: 100%;
}

a img{
	color:$primary-2;
}

i{
	font-style: italic;
}
.main-container{
	margin-top: 0px;
	max-width: 1600px;
	width: 980px;
	margin: 0 auto;
	@include rem('margin-top', 34px);
	height: auto;
}

.heading-wrapper{
	display: inline-block;
	margin: 0 auto;
	clear: both;
	width: 100%;
}

.heading1{
	@include f(30);
	text-align:center;
	text-transform:uppercase;
	@include rem('padding', 20px 0);
	color:$primary-2;
	@include rem('letter-spacing',3px);
}

.heading2{
	@include f(26);
	text-align:center;
	text-transform:uppercase;
	@include rem('padding', 10px 0 10px);
	color:$primary-2;
	@include rem('letter-spacing',3px);
}

.heading-white-line{
	border-bottom:1px solid #ffffff;
}

.heading3{
	@include f(15);
	text-align:center;
	text-transform:uppercase;
	@include rem('padding', 10px 0 15px);
	@include rem('letter-spacing',3px);
	color:$primary-2;
	width: 100%;

}

.heading4{
	font-weight: bold;
	@include f(10);
	color:$primary-2;	
    text-align: left;
    text-transform: uppercase;
    @include rem('padding',0px);
    @include rem('margin-bottom',10px);
    @include rem('letter-spacing',1.2px);
}

.heading5{
	@include brand-font;
	@include f(26);
	text-align:center;
	text-transform:uppercase;
	@include rem('padding', 30px 0);
	color:$primary-2;	
}
.heading6{
	@include brand-font;
	@include f(24);
	text-align:center;
	text-transform:uppercase;
	@include rem('padding', 30px 0);
	color:$primary-2;	
}

.body-wrapper{
	p{
		@include f(11);
		text-align:left;
		@include main-font;
		@include rem('padding', 15px 0);
		@include rem('letter-spacing', 1px);
		@include rem('line-height', 18px);
	}

	p.forgotpass-link {
		display: inline;
		font-family: Georgia, sans-serif;
		font-size: 12px;
		line-height: 14px;
		letter-spacing: normal;
	}

	.social-share{
		p{
			@include main-font;
		}
	}

	label{
		@include proxi-font();
		@include f(10);
	}
}

/*Special fonts*/

.lovefont {
	font-family: "CartierLoveTM" !important;
	display: inline-block !important;
}


