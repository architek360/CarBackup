}
#sortable { list-style-type: none; margin: 0; padding: 20px; width: 60%; }
	#sortable li { margin: 0 3px 10px 3px; padding: 0.4em; padding-left: 1.5em; font-size: 1.4em; height: 70px; cursor: move; background-color: grey;}
	//#sortable li span { position: absolute; margin-left: -1.3em; }

.edit-icon {
 	@include setSprite(27px, 29px, -730px, -134px);         
