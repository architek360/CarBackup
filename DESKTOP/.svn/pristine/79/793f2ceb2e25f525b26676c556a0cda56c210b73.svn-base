/**************************************************************************************
		DISPATCH LANDING PAGE SCSS

***************************************************************************************/
.dispatch-landing{ 
	background-color:$primary-1;
}
.dispatch-body{
	-background-color:$primary-1;
	max-width: 1600px;
	width: 100%;
	margin: 0 auto;
	min-height: 639px;
	overflow:hidden;
	.site-logo{
		@include rem('margin-top', 25px);
		@include rem('margin-bottom', 15px);
		text-align:center;
		position: relative;
	}
	.main-dispatch{
		@include rem('margin-top', 50px);
		text-align: center;
		@include rem('letter-spacing', 1px);
		display: inline-block;
		width: 100%;
		position: relative;
		.heading2{
				color: $primary-2;
				@include f(18);
				font-weight: normal;
				text-transform: uppercase;
				position: relative;
				letter-spacing: 0.2rem;
		}

		.continent{
			color: $primary-2;
			@include f(13);
			font-weight: bold;
			@include rem('margin-top', 20px);
			text-transform: uppercase;
			@include rem('line-height', 15px);
		}
		ul{
			li{
				list-style: none;
				@include rem('margin-top', 5px);
				text-transform: uppercase;
				a{
					color: $primary-3;
					@include f(11);
					font-weight: normal;
					text-decoration: none;
				}

				a:hover{
					color: $primary-2;
				}
			}
		}
		.left-col{
			width: 274px;
			float: left;
			@include rem('margin-left',230px);
			position: relative;
			.bag{
				background:url($path-images+'icons/bag.png') no-repeat center top;
				height: 20px;
				width: 23px;
				margin:4px auto 0;
			}
			.heading-wrapper:before{
				content: "";
				border-top: 1px solid $primary-69;
				left: 0;
				position: absolute;
				top: 19px;
				width: 50px;
			}

			.heading-wrapper:after{
				content: "";
				border-top: 1px solid $primary-69; 
				right: 0;
				position: absolute;
				top: 19px;
				width: 50px;
			}
		}

		.right-col{
			width: 274px;
			position: relative;
			float: right;
			@include rem('margin-right',230px);
			.heading-wrapper:before{
				content: "";
				border-top: 1px solid $primary-69;
				left: 0;
				position: absolute;
				top: 19px;
				width: 30px;
			}
			.heading-wrapper:after{
				content: "";
				border-top: 1px solid $primary-69;
				right: 0;
				position: absolute;
				top: 19px;
				width: 30px;
			}
		}
	}

	.dispatch-image{
		position:absolute; 
		left:0; 
		right:0; 
		top:0; 
		bottom:0; 
		margin:0 auto;
	}

}
.logout-icon{
	@include setSpriteDesktop(13px, 13px, -360px, -11px, transparent );
	@include rem(margin, 0 0 0 10px);
}

/*This class is for both server and client side messages. So the mixins were not used. The importnat is used to over ride all the styling for server side messages*/
