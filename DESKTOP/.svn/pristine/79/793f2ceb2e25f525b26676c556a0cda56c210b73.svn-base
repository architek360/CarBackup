}
.cross-button{
    position:absolute;
    top:7px;
    right:7px;
    width:20px;
    height:20px;
    @include setSprite(27px, 25px, -620px, -136px ); 
    background-repeat: no-repeat;
    cursor: pointer;

  }
.page-title{
	@include f(26);
