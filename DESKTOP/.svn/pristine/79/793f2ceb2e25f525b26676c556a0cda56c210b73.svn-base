.js-overlay {
	@include rem('top', 0px);




	.img-container {
	    position: absolute;
	    @include rem('top', 30px);
	    bottom: 0;
	    left: 0;
	    right: 0;
	    text-align:center;
	}



	.img-container:before {
	    content: ' ';
	    display: inline-block;
	    vertical-align: middle;
	    height: 100%;
	}



	img {
		opacity : 1;
		display : block;
		position: absolute;
		left : 0;
		right : 0;
		top  : 0;
