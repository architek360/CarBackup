				}
			}

			.nav-show{
				background: $primary-2;
				border-bottom: 1px solid $primary-63;

				.secondary-nav {
					line-height: 26px;
					a.sec-nav-link:hover{
						border-bottom: 1px solid $primary-1;
    					color: $primary-1;
					}

					a.sec-nav-link.active{
						border-bottom: 1px solid $primary-1;
    					color: $primary-1;
					}
				}
			}
