/*------------------------------------*\
    IE8 Bridal - Desktop CSS
\*------------------------------------*/


.push-over {
  background-color: white;
  -ms-filter: "alpha(opacity=80)";
}

.nav-show {
  border-top: 1px solid #bdbdbd;
  border-bottom: 1px solid #bdbdbd;
}

.nav-heading-cont .nav-show {
  filter: alpha(opacity=80);
  background: white;
}

.body-wrapper .header {
	filter: alpha(opacity=80);
  	background: #f0f0f0;
}
