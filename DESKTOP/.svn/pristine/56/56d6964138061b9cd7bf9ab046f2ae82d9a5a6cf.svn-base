/*------------------------------------*\
     Form Elements- Desktop CSS
\*------------------------------------*/
.no-js{
    /*TO BE IMPLEMENTED*/
}

.grid-bg {
    background: url("../../etc/designs/dmp/clientlibs_rbs_desktop/images/grid-layout.jpg") repeat-y 31px 0;
}

/** Button Styling **/
.red-more-button {
    padding-left: 1.0rem !important;
    display: inline-block;
    @include setSpriteDesktop(10px, 7px ,-431px ,-157px, transparent);
}

.cta--red-width-30{
    background:$primary-5;
    @include rem('padding', 15px 0);
    width:100%;
}

.cta--grey-pad-30 {
  background:$primary-13;
  @include rem('padding', 15px 0);
  width: 100%;
  text-align: center;
}

.more-button {
    @include main-font();
    display: block;
    width: 100%;
    @include rem('padding', 5px 10px);
    color:$primary-3;
    font-style: italic;
    @include f(12);
    letter-spacing: 0;

    .more-button-arrow{
        display: inline-block;
        @include setSpriteDesktop(10px, 7px ,-431px ,-159px, transparent);
    }
}

.arrow-show {
    @include setSpriteDesktop(10px, 6px ,-431px ,-158px, transparent);
    position: relative;
    @include rem('top', -2px);
    @include rem('margin-right', 5px);
}

.arrow-hide{
    @include setSprite(15px, 15px, -633px, -20px);
    position: relative;
    @include rem('top', 2px);
    @include rem('margin-right', 5px);
}


/** Form elements Styling **/
.form-select {
    background: transparent;
    @include rem('padding', 5px);
    @include f(13);
    outline: none;
    border: 0;
    width: 100%;
    color:$primary-3;
    @include rem('height', 56px);
    clear: both;
    overflow: hidden;
    @include setSpriteDesktop(auto, 56px, 288px, -358px, $primary-42);
    border: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    appearance: none;
    text-indent: 0.01px;
    text-overflow: '';
    font-style: normal;
    height: 28px;
    color:$primary-2;

    option{
        background:none;
        @include rem('font-size', 13px);
        @include rem('padding', 3px 5px);
        color:$primary-2;
   }
}

/*------------------------------------*\
    $LABEL  ELEMENTS
\*------------------------------------*/

.form-label {
    display: block;
    width: 100%;
    @include bold-font;
    @include f(10);
    letter-spacing: 1px;
    @include rem('padding', 0 0 5px 0);
    @include rem('margin', 0 0 5px 0);
    text-transform: uppercase;
    text-align: left;
    line-height: 20px;
}

.group-wrapper {
    
    > label {
        display: block;
        width: 100%;
        @include proxiBold-font();
        @include f(10);
        @include rem(letter-spacing, 1px);
        @include rem(margin-bottom, 5px);
        text-transform: uppercase;
        text-align:left;
    }
}

.cta-button {
    margin: 10px auto;
    cursor: pointer;  
    vertical-align: middle;   
    border: 0px none;   
    text-align: center;

    .input-arrow {
        display:inline;
        @include setSpriteDesktop(3px, 13px, -124px, -45px, transparent);
        position: relative;
        @include rem('top', -2px);
        @include rem('margin-right', 2px);
    }
    .cta-button__input {
        display:inline;
        border:none;
        background:none;
        @include f(12);
        font-family: proximanovaBold,"Helvetica Neue",Helvetica,Arial,sans-serif !important;
        color: #fff;
        @include rem('letter-spacing', 1px);
        text-decoration: none;
        text-transform: uppercase; 
        padding: 0;       
    } 

    &:hover{
        background:$primary-51;
    }
}

.cta--red {
    width: auto;
    background:$primary-5;
    @include rem('padding', 9px); 
}

.cta--red-width-auto-pad-20 {
     background:$primary-5;
}

.cta--grey {
    background:$primary-13;
    @include rem('padding', 9px);

    &:hover{
        background:$primary-95;

    }
}


.form-check {
    float: left;
    width: 50%;
    @include f(12);
    @include rem(margin, 12px 0 12px 0px);
    text-indent: 0px;

    .form-check__icon{
        position:relative;
        left:6px;
        width: 22px;
        @include rem(height, 20px); 
        display:inline-block;
        z-index:10;
        vertical-align:top;
    }

    input[type=checkbox] {  
        opacity:0;
        height:1px;
        width:1px;
        display:inline-block;
        filter:alpha(opacity=0); /* For IE8 and earlier */
        position: absolute;
    }

    input 
    {
        &:checked + span.form-check__icon
        {
            @include setSprite(22px, 20px, -746px, -46px);
            vertical-align: top;

        }
    }

    .form-check__wrapper {  
        position:relative;
        left:-20px;
        width: 20px;
        @include rem(height, 20px); 
        display: inline-block;
        background-color: $primary-34;
        border:solid 1px $primary-12;
        vertical-align:top;
    }

    .form-check-label{
        display:inline-block;
        width:83%;
    } 
}

.form-check-2 {
    display: block;
    @include f(22);
    @include brand-font;
    text-transform: uppercase;
    line-height: 31px;
    vertical-align: middle;

    .form-check__icon{
        position:relative;
        left:6px;
        width: 32px;
        @include rem(height, 32px); 
        display:inline-block;
        z-index:10;
        vertical-align:top;
        background-color: $primary-34;
        border:solid 1px $primary-12;
        float:right;
    }

    input[type=checkbox] {  
        display: none;  
    }

    input 
    {
        &:checked + span.form-check__icon
        {
            @include setSprite(32px, 32px, -457px, -83px);
            background-color: $primary-34;
            border:solid 1px $primary-12;
        }
    }
}

/* UNIFORM JAVASCRIPT */

/*------------------------------------*\
    $COMMON PROPERTIES FOR SPRITE
\*------------------------------------*/

.selector, .selector span, .checker span, .radio span, .uploader, .uploader span.action{
    background-image: url($path-images+'icons/sprite-desktop.png');
    background-repeat: no-repeat;
    -webkit-font-smoothing: antialiased;
}

.checkbox-wrapper{
    display: block;
    float: left;
    @include rem(margin, 5px 0); 

    .form-checkbox-label {
        @include rem('width', 400px); 
        display: block;
        float: left;
        color: $primary-2;
        text-transform: uppercase;
        max-width: 85%;
        margin-top: 5px;

        .checker{
            float:left;

            span{
                @include rem('width', 20px); 
                @include rem('height', 13px); 
                display: inline-block;
                @include rem(margin-top, 2px); 
                zoom: 1;
                text-align: center;
                vertical-align: top;
            }
/*          .checked {
                background-position: -34px -460px;
            }*/
            input{
                display: inline-block;
                @include rem(margin-top, 2px); 
                opacity: 0;
                filter: alpha(opacity=0);
                -moz-opacity: 0;
                border: none;
                background: none;
                display: inline-block;
                zoom: 1;
            }

            .wrapper-error{
                border: 1px solid #8f141a!important;
                background: #3b1112!important;
                height: 10px;
                width: 10px;
                @include rem(margin-right, 8px); 
            } 

        }
        .checkbox-name{
            @include main-font;
            @include f(12);
            @include rem('line-height', 16.8px);
            @include rem('margin-top', 1px);
            width: auto;
            text-align: left;
            float: left;
            margin-left: 0;
            text-transform: none;
        }
    }

    .form-radio-label {
        margin-right: 0;
        text-transform: uppercase;

        .radio-name {
            text-transform: uppercase;
            @include rem('margin-top',-0.5px);
            color: $primary-2;
            text-transform: uppercase;
            background:none;
            margin-right:10px;
            @include f(10);
        }
        .form-radio {
            @include rem('margin-top',-3px);
        }
    }   
}

.group-wrapper .radio, .radio {
    display: inline-block;
    position: static;
    right: auto;
    margin-top: -3px;

    span{
        width: 15px;
        height: 10px;
        display: inline-block;
        color: $primary-2;
        @include proxi-font();
        @include f(10);
        display: inline-block;
        margin-top: 2px;
        text-align: center;
        background-position:-54px -460px;
    }

    input{
        width: 10px;
        height: 10px;
        margin-top: 2px;
        opacity: 0;
        filter: alpha(opacity=0);
        -moz-opacity: 0;
        border: none;
        background: none;
        display: inline-block;
        zoom: 1;
        text-align: center;
        float: left;
    }
}

.radio-title{
    @include rem(letter-spacing, 1px);
    @include rem(margin-left, -18px);
    @include rem(padding-left, 18px); 
}

/*------------------------------------*\
    $INPUT ELEMENTS
\*------------------------------------*/

