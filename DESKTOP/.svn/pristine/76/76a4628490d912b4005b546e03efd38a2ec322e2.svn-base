.refine-selector .refine-selector__content .js-range-slider,#Price-slider,#carat-slider{
	background: url($path-images + "icons/sub-slider-price-range.png") no-repeat;
	border:none;
	margin:10px 7px;
	width:90%;
	

	.ui-slider-range{
		background: #b50606!important;
		height: 1px;
		top: 5px;
	}
	.ui-slider-handle{
		background: url($path-images + "icons/cursor-slider.png") no-repeat center center;
		border: none;
		height: 17px;
		width: 17px;
		z-index: 20;
	}
}
#Price-slider,#carat-slider{
	background: url($path-images + "icons/slider_bg.png") no-repeat 0 0;
	height: 8px;
	width: 470px;
	@include rem('margin-left', 49px);
	.ui-slider-range{
		background:none;
	}
