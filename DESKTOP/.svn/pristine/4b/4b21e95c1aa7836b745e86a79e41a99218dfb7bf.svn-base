		margin: 0 auto;
		@include main-font;
		font-size:13px;
		padding:20px;
		margin-top: 10px;
		span {
			display: block;
			text-align: center;
			padding: 4px;
		}

		.radio-bobj{
			display:inline-block;
			margin:0;
			vertical-align:top;

			.radio{
				line-height:160px;
				vertical-align:middle;
				span{
					width:4px;
				}
				input{
					width: 17px;
					height: 27px;
					margin-top: -11px;
					margin-left: -6px;
				}
			}
			span{
				display:inline-block;
			}
		}

		.address-bobj{
			display:inline-block;
			width:89%;
		}

		span.js-add-fname{
			text-align: right;			
			font-size:14px;
			display: inline-block;
			text-transform: uppercase;
		}
		span.js-add-lname{
			text-align: left;			
			font-size:14px;
			display: inline-block;
			text-transform: uppercase;
		}
		span.js-add-snum {
			text-align: right;			
			display: inline-block;
		}
		span.js-add-sname {
			text-align: left;
			display: inline-block;
		}
		span.js-add-pobox {
			text-align: left;
			display: inline-block;
		}
		span.name-row , span.address-row {
			display:block;
			padding: 0px;
		}
	}	
}

.visibility-none {
	visibility: hidden !important;
}
