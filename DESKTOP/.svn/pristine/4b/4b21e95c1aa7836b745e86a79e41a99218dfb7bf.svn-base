
#address-container {
		.message{
				font-family: "ProximaNova","Helvetica Neue",Helvetica,Arial,sans-serif;
				letter-spacing: .1em;
				text-transform: uppercase;
				font-size: 12px;
				font-weight: bold;
				font-weight: normal;
				
				text-align: center;
	}
	#address-select .select-wrapper{
		width:313px;
		margin-top:0;
		padding-top:0;
	}
	.addressListingDetailView .addressList .address p {
		line-height:20px;
	}
	.select-wrapper .selector {
		width:306px;

		span{
			width:309px;
		}
	}

	.txt, .comp-rich-text{
			@include proxi-font;
			border-top:1px solid $primary-11;
			text-align: center;

			p {
				background: $primary-1;
				@include rem('padding', 0 10px);
				@include f(14);
				position:relative;
				top:-11px;
				display:inline-block;
				@include rem('letter-spacing', 1px);
				ont-family: "ProximaNova","Helvetica Neue",Helvetica,Arial,sans-serif !important;
				font-size: 14px !important;
			}
	}

			div.errormessage {
			    color: #8F141A !important;
			    display: block;
			    font-family: Georgia,sans-serif !important;
			    font-size: 12px !important;
			    font-style: italic !important;
			    letter-spacing: 0 !important;
			    padding-top: 0.5rem;
			    text-align: left;
			    text-transform: none !important;
			    -webkit-animation-duration:1s;
			    -moz-animation-duration:1s;
			    -o-animation-duration:1s;
			    animation-duration:1s;
			    -webkit-animation-fill-mode:both;
			    -moz-animation-fill-mode:both;
			    -o-animation-fill-mode:both;
			    animation-fill-mode:both;
		}
}

#address-container + .txt, #address-container + .comp-rich-text{
			@include proxi-font;
			border-top:1px solid $primary-11;
			text-align: center;

			p {
				background: $primary-1;
				@include rem('padding', 0 10px);
				@include f(14);
				position:relative;
				top:-11px;
				display:inline-block;
				@include rem('letter-spacing', 1px);
				ont-family: "ProximaNova","Helvetica Neue",Helvetica,Arial,sans-serif !important;
				font-size: 14px !important;
			}
	}
	
	.addressListingDetailView {
			
		@include rem('width',313px);
		margin : 0 auto;
		@include rem('padding-bottom' , 22px);

		
		.addressList {
			
			display: none;
			color: white;
			text-align: center;
			border: 1px solid #333333;
			@include rem('padding' , 36px 15px 11px);

			.address {
					
					@include rem('padding-bottom', 37px);
					.address-details,p,.address-name-view{
						width: 265px;
						padding: 15px;
						margin:0 auto;
						vertical-align: middle;
						text-align: center;
						font-style: italic;
						line-height: 17px;
						font-size: 12px;	
						font-family: Georgia, sans-serif;
						padding: 0 ;
						p{
							text-align: center;
						}


				}
				h3 {
					display :  inline;
				}

				.heading3{
					display: none;
				}

				.message{
					display: none;
				}
			}

			
				
			.addressAction {
				border-top: 1px solid #333333;
				display: block;

	            .button{
	            	width:39%;
	        		@include f(12);
	        		//@include bold-font;
	        		font-weight: normal;
	        		@include proxi-font;
	        		text-transform:uppercase;
	        		display:inline-block;
	        		@include box-sizing(border-box);
	        		@include rem(padding, 15px 0 0);

	            	.delete-icon {
			       		@include setSprite(27px, 29px, -623px, -135px);           
						display: inline-block;
						@include rem('height', 15px);
						@include rem(margin, 0 0 0 5px);
						border-left:1px solid $primary-8;
						position:relative;
						top:-1px;

			         }
	            }
	        	 .edit_add{
	        		text-align:left;
	        		@include rem(margin-right, 10px);

	        		/* edit css*/

	        		/*@include setSprite(54px, 29px, -701px, -134px);         */
					display: inline-block;
					@include rem('height', 15px);
					@include rem(margin, 0 0 0 5px);
					border-left:0px solid $primary-8;
					position:relative;
					top:-1px;

	        	}

	        	.delete_add{
	        		text-align:left;
	        		@include rem(margin-left, 10px);

	        		/* delete button cass*/

	        		/*@include setSprite(75px, 29px, -580px, -135px);           */
					display: inline-block;
					@include rem('height', 15px);
					@include rem(margin, 0 0 0 5px);
					border-left:0px solid $primary-8;
					position:relative;
					top:-1px;

	        	}

	        	
			}


			     


			}

			.default{
				display: block;
			}
	}	
	
	
	

	/* delete */
	



