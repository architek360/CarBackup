.js-address-form ,.js-address-form-uk, .js-address-form-jp{

			
			background: $primary-1  url($path-images + 'icons/popin-bg.png') no-repeat top left;
			border: 1px solid $primary-11;
			display: block;
			float: left;
    		width: 100%;
    		@include box-sizing(border-box);
    		@include rem('padding',0px 30px 10px);
    		@include rem('margin-bottom',30px);
    		@include rem('margin-right',10px);
      		.form {
    			@include rem('width',445px);

    			margin : 5px auto 30px;
    		}
    		fieldset {
    			position : relative;
    			a {
    				color : $primary-114;
    				@include f(13);
    				@include main-font;
    				position : absolute;
    				bottom :  -113px;
    				right :  46%;
    			}
    		}

    		.button-wrapper .form-button {
				padding: 10px 15px 10px 23px;
				background-position: -416px -482px;
			}
    		.group-wrapper {
				margin: 2.5rem 0rem 1rem 0;

				> label {
					display: block;
				   	width: 100%;
					@include proxiBold-font();
					@include f(10);
					@include rem(letter-spacing, 1px);
					@include rem(margin-bottom, 5px);
					text-transform: uppercase;
					text-align:left;
				}

				.radio-wrapper {
					margin: 0.5rem 1rem 1rem 0;
				}
			}
    		
    		.addressform_address12 {
    			@include rem('margin-top' , 16px);
    		}
    		.addressform_address2 {
    			@include rem('margin-top' , 6px);
    		}
    		.addressform_address2 , .addressform_address12 {
    			float :  left;
    			@include rem('margin-right' , 117px);
    			
    			width: 23%;
    		}

    		.fn_fname,
    		.addressform_firstName1,
    		.addressform_address7,
    		.addressform_phone {
				width: 46%;
				.form-input {
					width : 100%;
				}
				
			}

			.addressLabel {
				width :100%;
				.form-input {
					width: 46%;
				}
			}

    		.addressform_address5 {
    			width : 47.1%;
    			@include rem('margin-top',6px);
    			
    		}

    		.form-input {
    			width :96%;
    		}
    		.form-label {
    			float: left;
    		}
    		.addressform_address5{
    				.form-input {
    					width : 100%;
    				}
    		}
    		.fieldset-title {
    				@include f(13);
    				@include proxiBold-font;
    				text-transform: uppercase;
    				margin-top: 10px;
    				@include rem('letter-spacing',1.3px);
    				@include rem('line-height',18.2px);
    				float: left;
    				 width: 100%;
    			}
    			.radio-wrapper {
    				@include rem('margin',25px 20px 10px 0);
				    width:auto;
				    display: block;
    				float: left;
  

					.radio {
				    	float: left;
				    }
				    .radio-name {
				    	text-transform: uppercase;
				    	@include proxiBold-font;
				    	@include f(10);
				    	@include rem('letter-spacing',1px);
				    	@include rem('line-height',14px);
				    	@include rem('margin-top',1px);
				    	text-align: left;
				    	float: left;
				    	width: auto;
				    	background: none
				    }
				}

    			.checkbox-wrapper {
    				display: block;
    				float: left;
    				@include rem('margin',10px 10px 10px 0);
				    width: 11%;
				    .form-radio-label {
				    	.form-radio {
				    		@include rem('margin-top' ,-4px);
				    	}
				    }
				    .checker {
				    	float: left;
				    }
				    .checkbox-name {
				    	text-transform: uppercase;
				    	@include proxiBold-font;
				    	@include f(10);
				    	@include rem('letter-spacing',1px);
				    	@include rem('line-height',14px);
				    	@include rem('margin-top',1px);
				    	
				    	text-align: left;
				    	float: left;

				    	p {
				    		padding : 0;
				    	}
				    }
				    
    			}
    			.addressform_principal {
				    	 
			    			clear : left;
					    	.checkbox-name {
								width : 94%;
								margin-top : 0;
							}
				}

    			.addressLabel {
    				clear : both;
    			}
    			
    			.select-wrapper {				
    				display: block;
				    float: left;
				    margin-right: 0px;
				    height: 50px;
				    margin-bottom: 20px;
				    margin-top: 0;

				    label {
				    	display: block;
				    }
    				.selector {
    					width: 445px !important;
    					display: block;

    					span {
    						width: 433px !important;
    					}
    				}

    			}

       			.show-button {
		    				padding-top: 0;
		    				@include rem('padding-bottom' ,20px);
		    				.form-button {
		    						margin : 0 auto;
		    				}
		    				
		    	}
		    			
    			.add-button {
				    
				    @include rem('margin',-8px 0 15px);
				    @include rem('padding',5px 0 10px);
					display:inline-block;
					width:100%;

    				a {
	    				@include setSpriteDesktop(auto, auto ,-431px ,-157px, $primary-1);
						color: $primary-2;
						float: left;
						@include main-font;
						@include f(12);
						font-style: normal;
						letter-spacing: normal;
						@include rem('margin-top', 5px);
						@include rem('padding-left', 10px);
					}
    			}


    		.last-form-wrapper {
    			border-bottom: 1px solid $primary-61;
    		}

    		
    		.half-width.full-block .form-input {
    			width: 49.2%;

    		}
    		.full-width {
    			width: 90.6%;
    		}
    		.full-block {
    			width: 90.6%;
    		}
    		.half-block {
    			width: 44.1%;
    		}

    		.fieldset-description {
    			@include f(11);
    			@include rem('margin-bottom', 15px);
    			@include main-font;
    			text-transform: none;
    			color: $primary-2;
    			@include rem('line-height', 15.4px);
    			display: inline-block;
    			width: 105%;
    		}
    		.form-address-selector {

    			height: 500px;
    			display: block;
    			clear :  both;
    			float: left;
    			@include rem('width', 43px);
    			@include rem('height', 142px);

    			
    			@include  rem('margin-left' , -55px);
    			.form-address-selector__radio {
    				width: 10px;
    				margin-right: 15px;
    				float: left;
    				margin-top: 27px;

					.radio span {
						background-position: -195px -432px;
						height: 20px;
						width: 20px
					}
					.radio span.checked {
						background-position: -218px -432px;
					}

    				.checkbox-wrapper {
    					@include rem('margin', 39px 1px 31px 0);
    					
    				}

    				.fn_grpaddr label {
    					font-size: 0px;

    				}
    			}
    			.form-address-selector__bracket {
    				border: 1px solid $primary-72;
    				border-right: none;
				    float: left;
				    height: 97px;
				    margin-top: 31px;
				    width: 10px;
    			}
    		}

    		.form-layout .select-wrapper {				
    				display: block;
				    

				    label, .selector  {
				    	display: block;
				    }
    		}
			.tooltip-div {
			    float: right;
			    margin: 0.5rem 0 0 1rem;
			    position: absolute;
			    width: 20px;
			}
			 .tooltip-div:first-child {
				right: 475px;
			    top: 329px;
			}
			.tooltip-div:last-child {
				right: 325px;
				top: 738px;
			}	
}



.default-button{ display: none;}

