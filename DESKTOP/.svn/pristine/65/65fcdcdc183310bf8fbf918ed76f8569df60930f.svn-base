			<h1 class="heading2">Find a boutique <br/> or retailer </h1>
			<div class="change-loc__search"> 
				<!--#set var="text-detail" value="geolocate me"-->
				<a class="cta <!--#echo var="cta--varaint" --> js-geolocateButton" alt="geolocate me"><span class="location"></span><!--#echo var="text-detail" --></a>
				<div class="store-locater__search">
					<!--#set var="label-name" value="Or search by city, zip code or state : " -->
					<!--#set var="mandatory" value="yes"-->
					<!--#set var="input-name" value="js-search-box-1" -->

					<!--#include virtual="../../components/form_elements/label.shtml" -->
				
					<input type="text" class="form-input js-search-box-1" id="js-search-box-1" placeholder="City,zipcode,state" />
					<a class="js-search-but-1" alt="find">
						<span class="store-locater__search__bg"></span>
					</a>
				</div>
				<div class="js-store-locator_error-msg-1">Please fill a zipcode, city or state </div>
				<div class="js-store-locator_error-msg-2">There is no result for your search, we suggest the following :</div>
				<div class="js-store-locator_error-msg-3">There is no result for your search.</div>
				<ul class="js-store-locater__error-suggest">
	                <li> <a href="#"> Paris, France </a> </li>
	                <li> <a href="#"> Parsberg, Germany </a> </li>
	               
	            </ul>
			    <div class="store-locater__find-button">
				      <!--#set var="text-detail" value="find a boutique"-->
