/*This class is for both server and client side messages. So the mixins were not used. The importnat is used to over ride all the styling for server side messages*/
span.error,label.error, .input-wrapper label.error {
    @include font-color($primary-20 !important);
    font-family: Georgia, sans-serif !important;
    font-style: italic !important;
    text-transform: none;
    font-size: 24px !important;
    display: block;
    padding-top: 5px !important;
    text-align: left;
    text-transform: none;
}

.formerror .form-select {
    border: 1px solid #8f141a !important;
    background-color: #3b1112 !important;
}

.js-zoom {
    cursor: pointer;
}

.error {
    text-transform: none !important;
}

.loaderImage {
    width: 40px;
    height: 40px;
    position: absolute;
    background-image: url($path-images + "icons/throbber.gif");
    background-repeat: no-repeat;
    background-size: cover;
    @include rem("margin-top", 7px);
}

.loading {
    background: url($path-images + "icons/throbber.gif") no-repeat;
    background-position: center;
    height: 100%;
}

.hide-product {
    display: none;
}

.display-none {
    display: none !important;
}

.opacity-none {
    opacity: 0 !important;
    filter: alpha(opacity = 0) !important;
}

/*
Fix For Iphone // 6th feb
 */

textarea,
select,
input[type="text"],
input[type="button"],
input[type="submit"] {
    -webkit-appearance: none;
    border-radius: 0;
}

.js-hide-content {
    display: none;
}

.js-show-content {
    display: block;
}

.error-zone {
    margin: 0 auto;
    color: #8f141a;
    font-weight: bold;
    text-align: center;
    margin-top: 20px;
    padding: 20px;
    display: block;
    width: 81.25%;
    border: 1px solid #4d4d4d;
    @include f(20);
    @include brand-font;
    span {
        margin-top: 5px;
        text-transform: uppercase;
        display: block;
        @include bold-font;
        margin-bottom: 10px;
    }
}

.error-div {
    @include rem(margin, 40px 0px);
    color: #ffffff;
    padding: 20px;
    display: block;
    @include rem("padding", 10px);
    @include linear-gradient(135deg, black 73%, #010000 0%, #460000 100%);
    border: 1px solid red;
    text-align: center;
    @include main-font;
    @include f(18);
    word-wrap: break-word;
    width: 95%;
    span {
        margin-top: 5px;
        text-transform: uppercase;
        display: block;
        @include bold-font;
        margin-bottom: 10px;
    }
}

