/**************************************************************************************
		DISPATCH LANDING PAGE SCSS

***************************************************************************************/
.dispatch-landing {
    background-color: $primary-1;
}

.dispatch-body {
    background: $primary-1 none;
    width: 100%;
    margin: 0 auto;
    min-height: 100%;
    overflow: hidden;
    .site-logo {
        @include rem("margin-top", 25px);
        @include rem("margin-bottom", 15px);
        text-align: center;
        position: relative;
    }
    .main-dispatch {
        margin: auto;
        @include rem("margin-top", 50px);
        text-align: center;
        @include rem("letter-spacing", 1px);
        display: block;
        padding: 30px 50px;
        position: relative;
        .heading2 {
            color: $primary-2;
            @include f(27);
            font-weight: normal;
            text-transform: uppercase;
            position: relative;
            letter-spacing: 0.2rem;
        }
        .continent {
            color: $primary-2;
            @include f(20);
            font-weight: bold;
            @include rem("margin-top", 20px);
            text-transform: uppercase;
            @include rem("line-height", 15px);
        }
        ul {
            li {
                list-style: none;
                @include rem("margin-top", 5px);
                text-transform: uppercase;
                a {
                    color: $primary-3;
                    @include f(16);
                    font-weight: normal;
                    text-decoration: none;
                }
                a:hover {
                    color: $primary-2;
                }
            }
        }
        .heading-wrapper {
            position: relative;
        }
        .heading-wrapper:before {
            content: "";
            border-top: 1px solid $primary-69;
            left: 0;
            position: absolute;
            bottom: 45px;
            width: 50px;
        }
        .heading-wrapper:after {
            content: "";
            border-top: 1px solid $primary-69;
            right: 0;
            position: absolute;
            bottom: 45px;
            width: 50px;
        }
        .left-col {
            position: relative;
            .bag {
                background: url($path-images + "icons/bag.png") no-repeat center top;
                height: 20px;
                width: 23px;
                margin: 4px auto 0;
            }
        }
        .right-col {
            position: relative;
        }
    }
    .dispatch-image {
        position: absolute;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        margin: 0 auto;
        width: 100%;
    }
}

