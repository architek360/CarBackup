/*------------------------------------*\
    Global -- MOBILE CSS
 
\*------------------------------------*/
/**
 * FRAMEWORK STYLES

 */

/*------------------------------------*\
    $GLOBAL STYLES
\*------------------------------------*/
* {
    outline: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

body {
    background-color: $primary-1;
}

html {
    -webkit-text-size-adjust: 100%;
}

a img {
    color: $primary-2;
}

.text-link {
    text-transform: uppercase;
    display: inline-block;
    @include brand-font;
    @include f(20);
}

a.disabled {
    pointer-events: none;
}

.main-container {
    margin-top: 92px;
}

.heading1 {
    @include brand-font;
    @include f(34);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 50px 0 30px);
    color: $primary-2;
}

.heading2 {
    @include brand-font;
    @include f(32);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 50px 0 30px);
    color: $primary-2;
}

.heading-white-line {
    border-bottom: 1px solid #ffffff;
}

.heading3 {
    @include brand-font;
    @include f(30);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 30px 0);
    color: $primary-2;
}

.heading4 {
    @include brand-font;
    @include f(28);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 30px 0);
    color: $primary-2;
}

.heading5 {
    @include brand-font;
    @include f(26);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 30px 0);
    color: $primary-2;
}

.heading6 {
    @include brand-font;
    @include f(24);
    text-align: center;
    text-transform: uppercase;
    @include rem("padding", 30px 0);
    color: $primary-2;
}

.body-wrapper {
    p {
        @include f(26);
        text-align: left;
        @include rem("padding", 25px 0);
        @include rem("line-height", 30px);
        /*line-height:1.6em;
        font-size:1.4em;*/
    }
    label {
        @include main-font;
        @include f(22);
    }
}

hr {
    background: $primary-2;
    border: none;
    height: 1px;
    @include rem("margin", 25px 0);
}

.grey-text {
    color: $primary-3;
}

.underline-text {
    text-decoration: underline;
}

/* TEXT ALIGNMENT PROPERTIES */
.text-center {
    text-align: center;
}

.text-left {
    text-align: left;
}

.text-right {
    text-align: right;
}

